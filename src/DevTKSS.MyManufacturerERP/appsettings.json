{
  "AppConfig": {
    "Title": "My Manufacturer ERP",
    "Environment": "Production"
  },
  "EtsyOAuth": {
    "LoginStartUri": "https://www.etsy.com/oauth/connect",
    "LogoutStartUri": "https://my-manufacturer-erp.com/logout",
    "LoginCallbackUri": "https://my-manufacturer-erp.com/callback",
    "AccessTokenKey": "access_token",
    "RefreshTokenKey": "refresh_token",
    "ExpiresInKey": "expires_in",
    "IdTokenKey": "user_id",
    "OtherTokenKeys": [
      {
        "ShopIdTokenKey": "shop_id"
      }
    ]
  },
  "LocalizationConfiguration": {
    "Cultures": [
      "es",
      "fr",
      "pt-BR",
      "en"
    ]
  },
  "EtsyOAuthOptions": {
    "Url": "https://openapi.etsy.com",
    "UseNativeHandler": true,

    "AuthorizationEndpoint": "https://www.etsy.com/oauth/connect",
    "TokenEndpoint": "https://api.etsy.com/v3/public/oauth/token",
    "RedirectUri": "https://my-manufacturer-erp.com/callback",
    "ClientId": "",
    "Scopes": [
      "listings_r",
      "listings_w",
      "shops_r",
      "shops_w"
    ]
  }
}