{
  "AppConfig": {
    "Title": "My Manufacturer ERP - Development",
    "Environment": "Development"
  },
  "OAuth": {
    "Url": "https://openapi.etsy.com/v3/",
    "UseNativeHandler": true,
    "TokenCacheOptions": {
      "AccessTokenKey": "AccessToken",
      "RefreshTokenKey": "RefreshToken",
      "IdTokenKey": "IdToken"
    },
    "OAuthClientOptions": {
      "ClientId": "your-keystring-from-user-secrets",
      "EndpointOptions": {
        "AuthorizationEndpoint": "https://www.etsy.com/oauth/connect",
        "TokenEndpoint": "/public/oauth/token",
        "UserInfoEndpoint": "/public/users/me"
      },
      "CallbackUri": "https://localhost:5001/etsy/callback",
      "Scopes": [
        "listings_r",
        "listings_w",
        "shops_r",
        "shops_w"
      ]
    },
    "UriTokenOptions": {
      "AccessTokenKey": "access_token",
      "RefreshTokenKey": "refresh_token",
      "IdTokenKey": "client_id",
      "OtherTokenKeys": [
        {
          "ShopIdTokenKey": "shop_id"
        }
      ]
    }
  },
  "SevdeskApiKeyClient": {
    "UseNativeHandler": true,
    "Url": "https://my.sevdesk.de/api/v1",
    "ApiKey": "your-api-key-from-user-secrets"

  },
  "ServerOptions": {
    "BindAddres4": "127.0.0.1",
    "BindAddres6": "::1",
    "Hostname4": "localhost",
    "Hostname6": "localhost",
    "Port": 5001
  }
} 

