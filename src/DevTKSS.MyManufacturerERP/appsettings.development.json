{
  "AppConfig": {
    "Title": "My Manufacturer ERP - Development",
    "Environment": "Development"
  },
  "EtsyOAuth": {
    "LoginStartUri": "https://www.etsy.com/oauth/connect",
    "LoginCallbackUri": "https://localhost:5001/etsy/callback"
  },
  "ApiClient": {
    "Url": "https://openapi.etsy.com",
    "ApiKey": "your-api-key-or-keystring-from-user-secrets",
    "UseNativeHandler": true
  },
  "EtsyOAuthOptions": {
    "Url": "https://openapi.etsy.com/v3/public/oauth",
    "RedirectUri": "/etsy/callback",
    "ClientID": "your-client-id-aka-keystring-from-user-secrets",
    "Scopes": [
      "listings_r",
      "listings_w",
      "shops_r",
      "shops_w"
    ]
  },
  "SevdeskApiKeyClient": {
    "UseNativeHandler": true,
    "Url": "https://my.sevdesk.de/api/v1",
    "ApiKey": "your-api-key-from-user-secrets"

  },
  "ServerOptions": {
    "RootUri": "http://localhost",
    "Port": 5001,
    "CallbackUri": "/etsy/callback"
  }

} 

