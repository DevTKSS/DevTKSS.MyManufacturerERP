{
  "AppConfig": {
    "Title": "My Manufacturer ERP - Development",
    "Environment": "Development"
  },
  "OAuth": {
    "Url": "https://openapi.etsy.com/v3/",
    "UseNativeHandler": true,
    "AccessToken": "",
    "RefreshToken": "",
    "IdToken": "",
    "EndpointOptions": {
      "AuthorizationEndpoint": "https://www.etsy.com/oauth/connect",
      "TokenEndpoint": "/public/oauth/token",
      "UserInfoEndpoint": "/public/users/me",
      "RedirectUri": "https://localhost:5001/etsy/callback"
    },
    "ClientId": "your-keystring-from-user-secrets",
    "Scopes": [
      "listings_r",
      "listings_w",
      "shops_r",
      "shops_w"
    ],
    "OtherParameters": [
      {
        "ShopIdTokenKey": "shop_id"
      }
    ]
  },
  "SevdeskApiKeyClient": {
    "UseNativeHandler": true,
    "Url": "https://my.sevdesk.de/api/v1",
    "ApiKey": "your-api-key-from-user-secrets"

  },
  "ServerOptions": {
    "RootUri": "http://localhost",
    "Port": 5001,
    "CallbackUri": "/etsy/callback"
  }
} 

