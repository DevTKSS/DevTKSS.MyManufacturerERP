{
  "SevdeskApiOidcAuthentication": { // Sevdesk v1 api I am still set to might not be available as they introduced a Breaking Change that made their product not usable properly for many Customers
    "Authority": "https://my.sevdesk.de/api/v1/", // Authority Unknown, as the token is provided
    "ClientId": "SEVDESK_CLIENT_ID",
    "ClientSecret": "SEVDESK_API_X_KEY",
    "Scope": "openid profile email",
    "RedirectUri": "https://localhost:5002/callback", // unknown as you will most likly not need it. if you dont have entrance to your token check out their Reference: https://api.sevdesk.de/#section/Authentication-and-Authorization
    "PostLogoutRedirectUri": "https://localhost:5002/logout-callback" // unknown
  },
  "EtsyApiOidcAuthentication": {
    "Authority": "https://api.etsy.com/v3/public/oauth/token",
    "ClientId": "ETSY_CLIENT_ID",
    "ClientSecret": "ETSY_API_X_KEY",
    "Scope": "address_r billing_r cart_r email_r favorites_r feedback_r listings_r listings_w profile_r shops_r shops_w transactions_r transactions_w",
    "RedirectUri": "https://localhost:5002/callback", // both uri have to get set in the configuration UI see this for lookup: https://www.etsy.com/developers/your-apps
    "PostLogoutRedirectUri": "https://localhost:5002/logout-callback"
  }
}
